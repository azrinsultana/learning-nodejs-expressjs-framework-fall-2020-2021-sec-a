<!DOCTYPE html>
<html>
<head>
	<title>Edit User</title>
</head>
<body>
	
	<form method="post">
		<fieldset>
			<legend>Edit User</legend>
			<table>
				<tr>
					<td>Username</td>
					<td><input type="text" name="username" value="<%=employee[0].username%>"></td>
					
				</tr>
				<tr>
					<td>Password</td>
					<td><input type="password" name="password" value="<%=employee[0].password%>"></td>
				</tr>
				
				<tr>
					<td>Employee Name</td>
					<td><input type="text" name="employeename" value="<%=employee[0].employeename%>"></td>
				</tr>
				<tr>
					<td>Company Name</td>
					<td><input type="text" name="companyname" value="<%=employee[0].companyname%>"></td>
				</tr>
				<tr>
					<td>Contatc Number</td>
					<td><input type="text" name="contactno" value="<%=employee[0].contactno%>"></td>
				</tr>
			
				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Submit"></td>
				</tr>
			</table>



		</fieldset>
	</form>
</body>
</html>